{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": "> 5%"
        },
        "useBuiltIns": "entry",
        // "useBuiltIns": true,
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
